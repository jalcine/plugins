prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@WNTRVOICE_LIB_DIR@
includedir=@WNTRVOICE_INCLUDE_DIR@

Name: Wintermute Voice (plugin for Wintermute)
Description: Library and development files for Wintermute's voice API.
Version: @GENERIC_LIB_VERSION@
URL: https://www.launchpad.net/wintermute
Requires: QtCore, Wintermute
Conflicts: Wintermute > @GENERIC_LIB_VERSION@
Libs: -L${libdir} -lwntrvoice
Libs.private: -lm
Cflags: -I${includedir}
